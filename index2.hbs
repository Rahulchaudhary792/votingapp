<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel = "stylesheet" href = "style2.css">
    <style>
      body {
    background-color: skyblue;
  }
  .party-div1 {
    width: 300px;
    height: 1590px;
    background-color: white;
    border-radius: 5px;
  }
  .p-div1 {
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .bjp-image {
    width: 150px;
    height: 150px;
  }
  .bjp-div {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 20px;
  }
  .l1 {
    display: flex;
    justify-content: center;
    align-items: center;
    font-family: Arial;
  }
  .p1 {
    display: flex;
    justify-content: center;
    align-items: center;
    font-family: Arial;
  }
  .p2 {
    display: flex;
    justify-content: center;
    align-items: center;
    font-family: Arial;
    position: relative;
    left: 60px;
  }
  .b-div {
    display: flex;
    justify-content: center;
    align-items: center;
    font-family: Arial;
  }
  button {
    color: white;
    background-color: rgb(52, 169, 215);
    border: none;
    border-radius: 5px;
    padding: 5px 20px;
    font-weight: bold;
  }
  .horizontal-line {
    margin-top: 20px;
    width: 280px;
  }
  @media (min-width: 300px) and (max-width: 400px) {
    .party-div1 {
      width: 300px;
      height: 1590px;
      background-color: white;
      border-radius: 5px;
      position: relative;
      left: 25px;
    }
  }
  @media (min-width: 0px) and (max-width: 300px) {
    .party-div1 {
      width: 300px;
      height: 1590px;
      background-color: white;
      border-radius: 5px;
      position: relative;
      left: 55px;
    }
  }
    </style>
  </head>
  <body>
    <div class="p-div1">
      <div class="party-div1">
        <div class="bjp-div">
          <img
            src="https://m.media-amazon.com/images/I/515uz47PkAL.jpg"
            class="bjp-image"
          />
        </div>
        <h4 class="l1">G.Kishan Reddy</h4>
        <p class="p1">Bharatiya Janata Party</p>
        <div class="b-div">
          <button class = "bjp-button">Vote</button>
        </div>
        <hr class="horizontal-line" />
        <div class="bjp-div">
          <img
            src="https://upload.wikimedia.org/wikipedia/commons/6/6c/Indian_National_Congress_hand_logo.svg"
            class="bjp-image"
          />
        </div>
        <h4 class="l1">Revanth Reddy</h4>
        <p class="p1">Indian National Congress</p>
        <div class="b-div">
          <button class = "congress-button">Vote</button>
        </div>
        <hr class="horizontal-line" />
        <div class="bjp-div">
          <img
            src="data:image/jpeg;base64,"
            class="bjp-image"
          />
        </div>
        <h4 class="l1">K.Chandrasekhar Rao</h4>
        <p class="p1">Bharatiya Rashtriya Samithi</p>
        <div class="b-div">
          <button class = "brs-button">Vote</button>
        </div>
        <hr class="horizontal-line" />
        <div class="bjp-div">
          <img
            src="https://upload.wikimedia.org/wikipedia/commons/b/b2/Indian_Election_Symbol_Cycle.png"
            class="bjp-image"
          />
        </div>
        <h4 class="l1">N.Chandrababu Naidu</h4>
        <p class="p1">Telugu Desam Party</p>
        <div class="b-div">
          <button class = "tdp-button">Vote</button>
        </div>
        <hr class="horizontal-line" />
        <div class="bjp-div">
          <img
            src="data:image/jpeg;base64,"
            class="bjp-image"
          />
        </div>
        <h4 class="l1">Asaduddin Owaisi</h4>
        <p class="p2">All India Majlis-e-Ittehadul Muslimeen(AIMIM)</p>
        <div class="b-div">
          <button class = aimim-button>Vote</button>
        </div>
      </div>
    </div>
    <script>
       let bjpButton = document.querySelector('.bjp-button');
let congressButton = document.querySelector('.congress-button');
let brsButton = document.querySelector('.brs-button');
let tdpButton = document.querySelector('.tdp-button');
let aimimButton = document.querySelector('.aimim-button');
let obj = {
    bjp: 0,
    congress: 0,
    brs: 0,
    tdp: 0,
    aimim: 0
}
bjpButton.addEventListener('click', () => {
    obj.bjp += 1;
    localStorage.bjp = obj.bjp;
    console.log(localStorage.bjp);
    alert('Successfully Voted');
});
congressButton.addEventListener('click', () => {
    obj.congress += 1;
    localStorage.congress = obj.congress;
    console.log(localStorage.congress);
    alert('Successfully Voted');
});
brsButton.addEventListener('click', () => {
    obj.brs += 1;
    localStorage.brs = obj.brs;
    console.log(localStorage.brs);
    alert('Successfully Voted');
});
tdpButton.addEventListener('click', () => {
    obj.tdp += 1;
    localStorage.tdp = obj.tdp;
    console.log(localStorage.tdp);
    alert('Successfully Voted');
});
aimimButton.addEventListener('click', () => {
    obj.aimim += 1;
    localStorage.aimim = obj.aimim;
    console.log(localStorage.aimim)
    alert('Successfully Voted');
});
    </script>
  </body>
</html>
